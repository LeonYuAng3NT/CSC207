package operator;

import operator.HTMLExtractor;

public class MockGoogleScholarHTMLExtractor implements HTMLExtractor{
  /** 
   *  A mock approach to obtain a HTML string 
   * 
   */
  @Override
  public String getHTML(String URL) throws Exception {
    if(URL.equals("sample1.html"))
    {
      return "sample1.html";
    }
    else if (URL.equals("sample2.html")){
      return "sample2.html";
    }
    return "";
  }

}
