package a2a.foundation;

import java.util.ArrayList;

public class DirectoryStack {

  private ArrayList<Directory> directoryStack = new ArrayList<>();

  /**
   * Construct a directory stack.
   */
  public DirectoryStack() {
  }

  /**
   * Detect the directory stack is empty or not.
   *
   * @return True if the directory stack is empty
   */
  public Boolean isEmpty() {

    return directoryStack.size() == 0;
  }

  /**
   * Getter of Directory stack.
   *
   * @return The directory stack
   */
  public ArrayList<Directory> getDirectoryStack() {

    return directoryStack;
  }

  /**
   * Setter of Directory stack.
   *
   * @param directoryStack The new Directory stack
   */
  public void setDirectoryStack(ArrayList<Directory> directoryStack) {

    this.directoryStack = directoryStack;
  }
}
